{
  "name": "tnp-iiit-kota",
  "version": "1.0.0",
  "private": true,
  "description": "The Landing Page of Training and Placement Cell - IIIT Kota",
  "repository": "https://github.com/brc-dd/tnp-iiit-kota",
  "license": "MIT",
  "author": "Divyansh Singh <divyansh.singh@ikcb.org> (https://github.com/brc-dd)",
  "scripts": {
    "audit:fix": "npm i --package-lock-only && npm update && npm audit fix && rm yarn.lock && yarn import && rm package-lock.json",
    "build": "next build",
    "dev": "next",
    "lint:code": "eslint . --ext .ts,.tsx --fix",
    "lint:staged": "lint-staged --allow-empty --concurrent 1",
    "prepare": "husky install",
    "preprettify:bash": "bash -c \"git ls-files '*.js' '*.ts' '*.jsx' '*.tsx' '*.json' | xargs sed -i ':a;N;$!ba;s/{\\s*\\n/{ /g'\"",
    "preprettify:pwsh": "pwsh -c \"git ls-files '*.js' '*.ts' '*.jsx' '*.tsx' '*.json' | ForEach-Object { (Get-Content $_ -Raw) -replace '{\\s*\\n', '{ ' | Out-File $_ -NoNewline }\"",
    "prettify": "prettier --write . --ignore-path ./.gitignore",
    "start": "next start",
    "type-check": "tsc"
  },
  "resolutions": {
    "css-what": "5.0.1"
  },
  "dependencies": {
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "cssnano-preset-advanced": "^5.1.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "prop-types": "^15.7.2",
    "sass": "^1.35.1",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0"
  }
}
